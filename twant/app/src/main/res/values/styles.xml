<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="tw_toast">
        <item name="stTextBold">false</item>
        <item name="stTextColor">#fff</item>
        <item name="stTextSize">17sp</item>
        <item name="stColorBackground">@color/tw_black</item>
        <item name="stSolidBackground">true</item>
        <item name="stLength">SHORT</item>
        <item name="stGravity">center</item>
        <item name="stRadius">4dp</item>
    </style>

    <style name="MenuButtonsSmall">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">#1565C0</item>
        <item name="fab_colorPressed">#2272CD</item>
        <item name="fab_colorRipple">#62B2FF</item>
    </style>
</resources>
