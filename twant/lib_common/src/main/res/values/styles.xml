<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tw_toast">
        <item name="stTextBold">false</item>
        <item name="stTextColor">#fff</item>
        <item name="stTextSize">17sp</item>
        <item name="stColorBackground">@color/tw_black</item>
        <item name="stSolidBackground">true</item>
        <item name="stLength">SHORT</item>
        <item name="stGravity">center</item>
        <item name="stRadius">4dp</item>
    </style>
    <!-- 自定义loading dialog -->
    <style name="common_loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>